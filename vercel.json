{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url",
    "REACT_APP_ADMIN_PATH": "@react_app_admin_path",
    "REACT_APP_ADMIN_PASSWORD_PATH": "@react_app_admin_password_path"
  }
}

